<h1>All Categories</h1>

<div class="container">
  <div class="row">
    <% @categories.each do |category| %>
    <div class="col-4">
      <a><%= link_to category.name, category_path(category) %>
        <% if session[:admin_id] != nil %>
          <%= button_to "Delete", category_path(category), method: :delete %>
        <% end %></a>
    </div>
    <% end %>
  </div>
</div>
